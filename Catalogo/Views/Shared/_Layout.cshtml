<!DOCTYPE html>
<html ng-app="CatalogoApp">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Cat&aacute;logo</title>
    <link href="../../Resources/bootstrap-3.3.5-dist/css/bootstrap.css" rel="stylesheet"
        type="text/css" />

    <link href="../../Resources/css/tabs.css" rel="stylesheet" type="text/css"/>
    <link href="../../Resources/css/simple-sidebar.css" rel="stylesheet" type="text/css" />
    <link href="../../Resources/js/ng-bootstrap.treeview/dist/abn_tree.css" rel="stylesheet" type="text/css" />
    <link href="../../Resources/css/treeview.css" rel="stylesheet" type="text/css" />
    <link href="../../Resources/css/special-glyphicons.css" rel="stylesheet" type="text/css" />
    
</head>
<body>
    <div class="row">
        @Html.Partial("_NavBar")
    </div>
    <div id="wrapper" ng-controller="TabsController as tabctrl">
        @if (Context.User.Identity.IsAuthenticated && true)
        {
            <div>
                @Html.Partial("_SideBar")
            </div>
        }
        <div class="row" >
            @RenderBody()
        </div>
    </div>
    <script src="../../Resources/js/jquery-2.1.4.js" type="text/javascript"></script>
    <script src="../../Resources/bootstrap-3.3.5-dist/js/bootstrap.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-animate.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-ui/ui-bootstrap-tpls.js" type="text/javascript"></script>
    <script src="../../Resources/js/validator.min.js" type="text/javascript"></script>
    <script src="../../Resources/js/sidebar-toggle.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/tabs.js" type="text/javascript"></script>
    <script src="../../Resources/js/ng-bootstrap.treeview/dist/abn_tree_directive.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/treeview.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/routing.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/usuario.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/grupo.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/programa.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/acceso.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/accesogrupo.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/usuariogrupos.js" type="text/javascript"></script>
    <script src="../../Resources/js/angular-modules/grupousuarios.js" type="text/javascript"></script>


    
</body>
</html>
